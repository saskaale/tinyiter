{
  "name": "tinyiter",
  "version": "1.0.0",
  "description": "An easy way to iterate and manipulate with javascript objects and arrays",
  "scripts": {
    "test": "mocha --compilers babel-register --require babel-polyfill",
    "compile": "rm ./lib/* && yarn compile:babel",
    "compile:babel": "babel --presets latest -d lib/ src/",
    "compile:minify": "npx google-closure-compiler --js=lib/index.js --compilation_level=SIMPLE_OPTIMIZATIONS --js_output_file=lib/index.min.js"
  },
  "main": "lib/index.js",
  "author": "Ales Saska",
  "license": "ISC",
  "devDependencies": {
    "assert": "^1.4.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "google-closure-compiler": "^20180805.0.0",
    "mocha": "^5.2.0"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators"
    ]
  },
  "dependencies": {}
}
